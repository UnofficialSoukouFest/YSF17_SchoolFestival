{
  "name": "@latimeria/ganoine",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./base.css": {
        "import": "./dist/extra/base.css",
        "default": "./dist/extra/base.css",
        "style": "./dist/extra/base.css",
        "asset": "./dist/extra/base.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "dev:css": "npm run genCss",
    "lint": "eslint .",
    "tsc": "tsc --noEmit",
    "genCss": "hcm -o generated/hcm src/**/*.module.css --localsConvention camelCaseOnly --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@latimeria/eslint-config": "*",
    "@rsbuild/plugin-react": "^1.3.2",
    "@rslib/core": "^0.10.4",
    "@tsconfig/recommended": "^1.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "happy-css-modules": "^4.0.0",
    "react": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@latimeria/design-token": "^0.1.7",
    "clsx": "^2.1.1",
    "react-aria": "^3.41.1",
    "react-aria-components": "^1.10.1",
    "react-icons": "^5.5.0"
  }
}
