{
  "name": "@latimeria/ganoine",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./base.css": {
      "import": "./dist/extra/base.css",
      "default": "./dist/extra/base.css",
      "style": "./dist/extra/base.css",
      "asset": "./dist/extra/base.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "dev:css": "pnpm run genCss",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "tsc": "tsc --noEmit",
    "genCss": "hcm -o generated/hcm src/**/*.module.css --localsConvention camelCaseOnly --watch",
    "gen": "turbo gen ganoine"
  },
  "devDependencies": {
    "@latimeria/eslint-config": "workspace:*",
    "@rsbuild/plugin-react": "catalog:",
    "@rslib/core": "catalog:",
    "@storybook/react": "^9.1.2",
    "@tsconfig/recommended": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "happy-css-modules": "^4.0.0",
    "postcss": "^8.5.6",
    "react": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "dependencies": {
    "@latimeria/design-token": "catalog:",
    "clsx": "^2.1.1",
    "react-aria": "catalog:",
    "react-aria-components": "catalog:",
    "react-icons": "catalog:"
  }
}
