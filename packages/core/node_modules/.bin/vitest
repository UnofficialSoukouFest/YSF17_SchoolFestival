#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/YSF17_SchoolFestival/YSF17_SchoolFestival/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.15.31_@vitest+browser@3.2.4_happy-dom@1_8cddecd1f94bba57431873e177c2caf5/node_modules/vitest/node_modules:/home/runner/work/YSF17_SchoolFestival/YSF17_SchoolFestival/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.15.31_@vitest+browser@3.2.4_happy-dom@1_8cddecd1f94bba57431873e177c2caf5/node_modules:/home/runner/work/YSF17_SchoolFestival/YSF17_SchoolFestival/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/YSF17_SchoolFestival/YSF17_SchoolFestival/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.15.31_@vitest+browser@3.2.4_happy-dom@1_8cddecd1f94bba57431873e177c2caf5/node_modules/vitest/node_modules:/home/runner/work/YSF17_SchoolFestival/YSF17_SchoolFestival/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@22.15.31_@vitest+browser@3.2.4_happy-dom@1_8cddecd1f94bba57431873e177c2caf5/node_modules:/home/runner/work/YSF17_SchoolFestival/YSF17_SchoolFestival/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
