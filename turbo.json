{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "storybook-static/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint": {},
    "format": {},
    "syncPublic": {
      "cache": false
    }
  }
}
